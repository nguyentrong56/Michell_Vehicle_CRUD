,OAXf,EAWwB;AAC3BhB,MAAAA,KAAK,CAACC,QAAN,CAAemC,IAAf,CAAoBpB,OAApB;AACH,KAbM;AAePqB,IAAAA,cAfO,0BAeQrC,KAfR,EAecgB,OAfd,EAesB;AACzB,UAAMe,KAAK,GAAG/B,KAAK,CAACC,QAAN,CAAe+B,SAAf,CAAyB,UAAAC,CAAC;AAAA,eAAIA,CAAC,CAACN,EAAF,IAAQX,OAAO,CAACW,EAApB;AAAA,OAA1B,CAAd;AACA3B,MAAAA,KAAK,CAACC,QAAN,CAAeqC,MAAf,CAAsBP,KAAtB,EAA4B,CAA5B;AACH;AAlBM;AAxEe,CAAf,CAAf","sourcesContent":["import axios from 'axios'\r\nimport Vuex from 'vuex'\r\nimport Vue from 'vue'\r\nimport {apiURL} from \"@/const.js\"\r\n\r\nVue.use(Vuex)\r\n//to handle state\r\n\r\nexport default new Vuex.Store({\r\n    state: {\r\n        vehicles: []\r\n    },\r\n    //to handle state\r\n    getters: {\r\n        allVehicles: (state) => state.vehicles\r\n    },\r\n\r\n    //to handle actions \r\n    actions: {\r\n        getVehicles({ commit }) {\r\n            const url =  process.env.VUE_APP_API_URL +'/vehicle' \r\n            axios\r\n                .get(url)\r\n                .then(response => {\r\n                    commit('SET_VEHICLES', response.data)\r\n                })\r\n        },\r\n\r\n        updateVehicle({ commit }, vehicle) {\r\n            const url = `https://localhost:44396/api/vehicles`\r\n\r\n            axios\r\n                .put(url, vehicle)\r\n                .then(response => {\r\n                    commit('UPDATE_VEHICLE', response.data)\r\n                }\r\n\r\n                )\r\n                .catch(err => alert(err.response.data))\r\n        },\r\n\r\n        createVehicle({ commit }, vehicle) {\r\n            const url = `https://localhost:44396/api/vehicles`\r\n            axios\r\n                .post(url, vehicle)\r\n                .then(response => {\r\n                    commit('CREATE_VEHICLE', response.data)\r\n                }\r\n\r\n                )\r\n                .catch(err => alert(err.response.data))\r\n\r\n        },\r\n\r\n        searchVehicle({ commit }, vehicle) {\r\n            const url = `https://localhost:44396/api/vehicles`\r\n            axios\r\n                .get(url, {params:vehicle})\r\n                .then(response => {\r\n                    commit('SET_VEHICLES', response.data)\r\n                }\r\n\r\n                )\r\n                .catch(err => alert(err.response.data))\r\n        },\r\n\r\n        deleteVehicle({commit},vehicle){\r\n            const url = `https://localhost:44396/api/vehicles/`\r\n            axios\r\n                .delete(url + vehicle.Id)\r\n                .then(response => {\r\n                    commit('DELETE_VEHICLE', response.data)\r\n                }\r\n\r\n                )\r\n                .catch(err => alert(err.response.data))\r\n        }\r\n    },\r\n\r\n\r\n    mutations: {\r\n        SET_VEHICLES(state, vehicles) {\r\n            state.vehicles = vehicles\r\n        },\r\n\r\n        UPDATE_VEHICLE(state, vehicle) {\r\n            const index = state.vehicles.findIndex(v => v.Id == vehicle.Id)\r\n            Vue.set(state.vehicles, index, vehicle)\r\n\r\n        },\r\n\r\n        CREATE_VEHICLE(state, vehicle) {\r\n            state.vehicles.push(vehicle)\r\n        },\r\n\r\n        DELETE_VEHICLE(state,vehicle){\r\n            const index = state.vehicles.findIndex(v => v.Id == vehicle.Id)\r\n            state.vehicles.splice(index,1)\r\n        }\r\n    },\r\n\r\n\r\n\r\n}\r\n)\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"]}]}