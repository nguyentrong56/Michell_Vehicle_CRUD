AAA,CAAA,QAAA,IAAA,MAAA,CAAA,IAAA,CAAA,mBAAA,CAAA;AACA,aAAA,MAAA;AACA,KAPA;AASA;AACA,IAAA,WAVA,yBAUA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA,CAAA,OAAA,CAAA,KAAA,CAAA,MAAA,EAAA,OAAA,MAAA;AACA,OAAA,KAAA,EAAA,CAAA,OAAA,CAAA,KAAA,CAAA,QAAA,IAAA,MAAA,CAAA,IAAA,CAAA,oBAAA,CAAA;AACA,aAAA,MAAA;AACA,KAfA;AAiBA;AACA,IAAA,UAlBA,wBAkBA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,MAAA,EAAA,OAAA,MAAA;AACA,OAAA,KAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,IAAA,MAAA,CAAA,IAAA,CAAA,kBAAA,CAAA;AACA,OAAA,KAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA,IACA,MAAA,CAAA,IAAA,CAAA,sCAAA,CADA;AAEA,aAAA,MAAA;AACA;AAzBA,GAtBA;AAkDA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,MAFA,oBAEA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,EAAA,CAAA,MAAA;;AADA,oBAEA,KAAA,CAAA,EAAA,CAAA,QAFA;AAAA;AAAA;AAAA;;AAGA,gBAAA,KAAA,CAAA,SAAA,CAAA,KAAA;;AAHA;AAAA,uBAIA,KAAA,CAAA,MAAA,CACA,QADA,CACA,eADA,EACA,KAAA,CAAA,OADA,EAEA,IAFA,CAGA,KAAA,CAAA,SAAA,CAAA,MAAA,EAHA,EAIA,KAAA,CAAA,KAAA,CAAA,OAAA,CAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,kBAAA,KAAA,CAAA,KAAA,CAAA;AACA,iBAPA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAfA;AAiBA;AACA,IAAA,MAlBA,oBAkBA;AACA,WAAA,KAAA,CAAA,OAAA;AACA;AApBA;AAlDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"modal-backdrop\">\r\n    <v-flex xs10 md10 lg4 offset-xs1 offset-sm3 offset-md1 offset-lg4>\r\n      <v-card>\r\n        <form>\r\n          <v-card-title primary-title>\r\n            <span>\r\n              <h2>Create Vehicle</h2>\r\n            </span>\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-text-field\r\n              v-model=\"vehicle.Make\"\r\n              :error-messages=\"makeErrors\"\r\n              label=\"Make\"\r\n              required\r\n              @input=\"$v.vehicle.Make.$touch()\"\r\n              @blur=\"$v.vehicle.Make.$touch()\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"vehicle.Model\"\r\n              :error-messages=\"modelErrors\"\r\n              label=\"Model\"\r\n              required\r\n              @input=\"$v.vehicle.Model.$touch()\"\r\n              @blur=\"$v.vehicle.Model.$touch()\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"vehicle.Year\"\r\n              :error-messages=\"yearErrors\"\r\n              label=\"Year\"\r\n              required\r\n              @input=\"$v.vehicle.Year.$touch()\"\r\n              @blur=\"$v.vehicle.Year.$touch()\"\r\n            ></v-text-field>\r\n\r\n            <v-btn class=\"mr-4\" @click=\"submit\"> submit </v-btn>\r\n            <v-btn @click=\"cancel\"> Cancel </v-btn>\r\n          </v-card-text>\r\n        </form>\r\n      </v-card>\r\n    </v-flex>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { validationMixin } from \"vuelidate\";\r\nimport { required, between } from \"vuelidate/lib/validators\";\r\nexport default {\r\n  name: \"CreateVehicleModal\",\r\n  mixins: [validationMixin],\r\n  // Define validations for empty Make, empty Model, empty and range of Year\r\n  validations: {\r\n    vehicle: {\r\n      Make: { required },\r\n      Model: { required },\r\n      Year: { required, between: between(1950, 2050) },\r\n    },\r\n  },\r\n\r\n  data: function () {\r\n    return {\r\n      vehicle: {\r\n        Make: \"\",\r\n        Model: \"\",\r\n        Year: \"\",\r\n      },\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    // Error checking for empty Make\r\n    makeErrors() {\r\n      const errors = [];\r\n      if (!this.$v.vehicle.Make.$dirty) return errors;\r\n      !this.$v.vehicle.Make.required && errors.push(\"Make is required.\");\r\n      return errors;\r\n    },\r\n\r\n    // Error checking for empty model\r\n    modelErrors() {\r\n      const errors = [];\r\n      if (!this.$v.vehicle.Model.$dirty) return errors;\r\n      !this.$v.vehicle.Model.required && errors.push(\"Model is required.\");\r\n      return errors;\r\n    },\r\n\r\n    // Error checking for empty year and range of year allowed\r\n    yearErrors() {\r\n      const errors = [];\r\n      if (!this.$v.vehicle.Year.$dirty) return errors;\r\n      !this.$v.vehicle.Year.required && errors.push(\"Year is required\");\r\n      !this.$v.vehicle.Year.between &&\r\n        errors.push(\"Year has to be between 1950 and 2050\");\r\n      return errors;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    // Method to dispatch create new vehicle command to Vuex store.js\r\n    async submit() {\r\n      this.$v.$touch();\r\n      if (!this.$v.$invalid) {\r\n        this.$Progress.start()\r\n       await this.$store\r\n          .dispatch(\"createVehicle\", this.vehicle)\r\n          .then(\r\n              this.$Progress.finish(),\r\n              this.$emit(\"close\"))\r\n          .catch((error) => {\r\n            alert(error);\r\n          });\r\n      }\r\n    },\r\n\r\n    // To close the modal\r\n    cancel() {\r\n      this.$emit(\"close\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal-backdrop {\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background-color: rgba(0, 0, 0, 0.3);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.modal {\r\n  background: #ffffff;\r\n  box-shadow: 2px 2px 20px 1px;\r\n  overflow-x: auto;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.modal-header,\r\n.modal-footer {\r\n  padding: 15px;\r\n  display: flex;\r\n}\r\n.modal-header {\r\n  border-bottom: 1px solid #eeeeee;\r\n  color: #4aae9b;\r\n  justify-content: space-between;\r\n}\r\n.modal-footer {\r\n  border-top: 1px solid #eeeeee;\r\n  justify-content: flex-end;\r\n}\r\n.modal-body {\r\n  position: relative;\r\n  padding: 20px 10px;\r\n}\r\n.btn-close {\r\n  border: none;\r\n  font-size: 20px;\r\n  padding: 20px;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n  color: #4aae9b;\r\n  background: transparent;\r\n}\r\n.btn-green {\r\n  color: white;\r\n  background: #4aae9b;\r\n  border: 1px solid #4aae9b;\r\n  border-radius: 2px;\r\n}\r\n.modal-fade-enter,\r\n.modal-fade-leave-active {\r\n  opacity: 0;\r\n}\r\n.modal-fade-enter-active,\r\n.modal-fade-leave-active {\r\n  transition: opacity 0.5s ease;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}