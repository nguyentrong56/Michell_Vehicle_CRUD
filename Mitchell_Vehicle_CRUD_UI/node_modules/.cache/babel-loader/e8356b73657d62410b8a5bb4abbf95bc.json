d} = validators\r\nconst validationMixin = vuelidate.validationMixin\r\nexport default {\r\n    name:\"CreateVehicle\",\r\n    mixins:[validationMixin],\r\n    validation: {\r\n        make: {required},\r\n        model:{required},\r\n        year:{required, range:range(1950,2050)},\r\n\r\n    },\r\n\r\n    data(){\r\n        return{\r\n            vehicle:{\r\n                Make: \"\",\r\n                Model:\"\",\r\n                Year:\"\"\r\n            }\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        makeErrors(){\r\n            const error = []\r\n            if(!this.$v.make.$dirty) return errors\r\n            !this.$v.make.required && errors.push('Make is required.')\r\n            return errors\r\n        },\r\n\r\n        modelErrors(){\r\n            const errors = []\r\n            if(!this.$v.model.$dirty) return errors\r\n            !this.$v.model.required && errors.push('Model is required.')\r\n            return errors\r\n        },\r\n\r\n        yearErrors(){\r\n            const errors = []\r\n            if(!this.$v.year.$dirty) return errors\r\n            !this.$v.year.required && error.push('Year is required')\r\n            !this.$v.year.range && error.push(\"Year has to be between 1950 and 2050\")\r\n            return errors\r\n        },\r\n\r\n    },\r\n\r\n    methods:{\r\n        async submit(){\r\n            const url = `https://localhost:44396/api/vehicles`\r\n            this.$v.touch()\r\n            await axios\r\n            .post(url, this.vehicle )\r\n            .then(response => alert(response.data))\r\n        }\r\n    }\r\n\r\n\r\n\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}